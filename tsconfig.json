{
  "compilerOptions": {
    "target": "ES2021",
    "module": "NodeNext",  // Altere para 'NodeNext'
    "moduleResolution": "NodeNext",  // Mantenha 'NodeNext'
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
